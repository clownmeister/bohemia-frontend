{
    "name": "clownmeister/bohemia",
    "description": "Frontend for bohemia-api.",
    "license": "proprietary",
    "type": "project",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "monolog/monolog": "2.3.*",
        "php-di/php-di": "6.3.*",
        "slim/psr7": "1.5.*",
        "slim/slim": "4.10.*",
        "slim/twig-view": "3.3.*"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "1.0.*",
        "phpspec/prophecy-phpunit": "2.0.*",
        "phpstan/extension-installer": "1.1.*",
        "phpstan/phpstan": "1.4.*",
        "phpunit/phpunit": "9.5.*",
        "squizlabs/php_codesniffer": "3.6.*",
        "symfony/var-dumper": "^5.3"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Clownmeister\\Bohemia\\": "src/"
        }
    },
    "scripts": {
    }
}
